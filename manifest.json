{
   "manifest_version":2,
   "name":"Ecommerce Widget",
   "description":"A dummy extension made by Jatin Kumar for Ecommerce Widget",
   "version":"1.0.0",
   "icons":{"128":"icon_128.png"},
   "browser_action":{
     "default_icon":"icon.png",
     "default_popup":"popup.html"
   },
   "content_scripts": [ {
    "js": [ "jquery.min.js", "popup.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
   "permissions":["activeTab"]
}